    "import random \n",
    "\n",
    "def dice():\n",
    "    die1:int = random.randint(1,6)\n",
    "    die2:int = random.randint(1,6)\n",
    "    total:int = die1 + die2\n",
    "    print(\"First die: \"+ str(die1))\n",
    "    print(\"Second die: \"+ str(die2))\n",
    "    print(f\"total of two dice is {total}\")\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    dice()\n",
    "\n"
   ]
  }
